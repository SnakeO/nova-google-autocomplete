{
    "name": "snakeo/nova-google-autocomplete",
    "description": "A Laravel Nova Google autocomplete field.",
    "keywords": [
        "yieldstudio",
        "laravel",
        "nova",
        "google",
        "autocomplete",
        "address"
    ],
    "homepage": "https://github.com/SnakeO/nova-google-autocomplete",
    "license": "MIT",
    "authors": [
        {
            "name": "Emiliano Tisato",
            "email": "emilianotisato@gmail.com",
            "homepage": "https://github.com/emilianotisato",
            "role": "Creator"
        },
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr",
            "homepage": "https://yieldstudio.fr",
            "role": "Fork maintener"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "SnakeO\\NovaGoogleAutocomplete\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SnakeO\\NovaGoogleAutocomplete\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8"
    }
}
